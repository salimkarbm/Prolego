{
  "name": "prolego",
  "version": "1.0.0",
  "description": "Prolego is a Robust web application for predicting student academic performance, it offers to contribute to the reduction of academic dropout and failure in educational institutes by predicting student's academic performance including drop off rate, academic success, to identify students at risk at an early stage of their academic path, so that strategies to support them can be put into place, in order to help Educators not only to focus more on bright students but also to initially identify students with low academic achievement and find ways to support them.",
  "main": "server.js",
  "scripts": {
    "start": "node src/server.ts",
    "postinstall": "tsc",
    "lint": "eslint --ext .ts . --fix",
    "format": "npm run lint && npm run prettier",
    "prettier": "prettier --config .prettierrc --ignore-path .gitignore  --write './src/**/*.+(js|ts)'",
    "jasmine": "jasmine",
    "test": "npm run tsc && NODE_ENV=test npm run jasmine",
    "test-db": "NODE_ENV=test db-migrate --env test up",
    "drop-test-db": "NODE_ENV=test db-migrate --env test up && jasmine && db-migrate db:drop test",
    "dev": " npm run tsc && nodemon --files ./src/server.ts",
    "prod": "NODE_ENV=production nodemon ./dist/server.js",
    "tsc": "tsc",
    "deploy": "git add . && git commit -m Heroku && git push heroku main"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/salimkarbm/Prolego.git"
  },
  "author": "stutern",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/salimkarbm/Prolego/issues"
  },
  "homepage": "https://github.com/salimkarbm/Prolego#readme",
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.102.0",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "express-validator": "^6.14.1",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.5",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^4.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.34",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine": "^4.0.2",
    "jasmine-spec-reporter": "^7.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "supertest": "^6.2.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": "^14"
  }
}
